{
    "name": "jpnut/laravel-typescript-codegen",
    "description": "Typescript Code Generation for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jamie Nuttall",
            "email": "nuttalljamie@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^9.0",
        "illuminate/routing": "^9.0",
        "illuminate/support": "^9.0",
        "phpdocumentor/reflection-docblock": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "JPNut\\CodeGen\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JPNut\\CodeGen\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JPNut\\CodeGen\\CodeGenServiceProvider"
            ]
        }
    }
}
